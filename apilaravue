#!/usr/bin/env php
<?php

if (file_exists(__DIR__.'/../../autoload.php')) {
    require __DIR__.'/../../autoload.php';
} else {
    require __DIR__.'/vendor/autoload.php';
}

$app = new Symfony\Component\Console\Application('Apilaravue Installer', '1.3.7');
$app->add(new Apilaravue\Installer\Console\NewCommand);

$app->run();
